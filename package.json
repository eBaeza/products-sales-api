{
  "name": "products-sales-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "sequelize": "node_modules/.bin/sequelize",
    "migrate": "npm run sequelize db:migrate",
    "migrate:v2": "npm run sequelize db:migrate -- --env development_2",
    "migrate:undo": "npm run sequelize db:migrate:undo:all",
    "migrate:v2:undo": "npm run sequelize db:migrate:undo:all -- --env development_2"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "mysql2": "^1.3.5",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.1.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "nodemon": "^1.11.0",
    "sequelize-cli": "^2.7.0"
  }
}
